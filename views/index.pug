doctype html
html(lang="en")
  head
    title= FoodiGo
    link(rel="stylesheet", href="https://fonts.googleapis.com/css?family=Roboto")
    link(rel="stylesheet", href="http://localhost:3000/style.css")
    link(rel="stylesheet", href="http://localhost:8001/style.css")
    link(rel="stylesheet", href="http://localhost:3400/style.css")
    link(rel="stylesheet", href="http://localhost:8000/style.css")
  body
    div(id="prox-header-top")
    div(id="prox-header")
      img(alt="logo", id="prox-logo", src="https://s3-us-west-1.amazonaws.com/foodigoiconlib/FoodiGo.png")
    div(id="prox-header-bottom")
    div(id="prox-app")
      div(id="prox-top")
        div(id="prox-title-map")
        div(id="prox-photos")
      div(id="prox-bottom")
        div(id="prox-reviews")
        div(id="prox-sidebar")
    script(crossorigin src="https://unpkg.com/react@16/umd/react.development.js").
    script(crossorigin src="https://unpkg.com/react-dom@16/umd/react-dom.development.js").
    script(src="http://localhost:8001/bundle.js")
    script(src="http://localhost:3000/bundle.js")
    script(src="http://localhost:3001/bundle.js")
    script(src="http://localhost:3400/bundle.js")
    script.
      ReactDOM.render(React.createElement(TitleMap, {id: #{id} }, null), document.getElementById("prox-title-map"))
    script.
      ReactDOM.render(React.createElement(Pictures, {id: #{id} }, null), document.getElementById("prox-photos"))
    script.
      ReactDOM.render(React.createElement(ReviewApp, {restaurantId: #{id} }, null), document.getElementById("prox-reviews"))
    script.
      ReactDOM.render(React.createElement(Sidebar, {id: #{id} }, null), document.getElementById("prox-sidebar"))
